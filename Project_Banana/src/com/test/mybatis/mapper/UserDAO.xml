<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  
<mapper namespace="com.banana.user.IUserDAO">
	
	<select id="getBUser" resultType="java.lang.String">
	SELECT B_USER_CODE
	FROM JOIN
	WHERE ID =#{id}
	</select>
	
	<select id="getId" resultType="java.lang.String">
	SELECT ID
	FROM JOIN
	WHERE B_USER_CODE =#{b_user_code}
	</select>		
	
	
	
</mapper>  

